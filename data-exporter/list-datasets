#!/usr/bin/env python3
from cassandra.cluster import Cluster

cluster = Cluster(control_connection_timeout=10.0, connect_timeout=15.0)
session = cluster.connect('waggle')
rows = list(session.execute('SELECT DISTINCT node_id, date FROM sensor_data_raw'))
cluster.shutdown()

for row in rows:
    if not row.node_id:
        continue
    if not row.date:
        continue
    print(row.node_id, row.date, flush=True)
